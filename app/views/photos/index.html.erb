<ul>
  <% @photos.each do |photo|%>
  <li>
    <%= photo.title %>
    <%= link_to 'ShowME!', photo_path(photo) %>
    <%= link_to 'Edit', edit_photo_path(photo) %>
    <%= link_to 'Destroy', photo_path(photo), method: :delete, data:{confirm: "Are you sure?"} %>
  </li>
  <% end %>
</ul>

<%= link_to 'New photo', new_photo_path %>